<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
xmlns:tns="http://www.example.org/NewWSDLFile/" 
xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
name="NewWSDLFile" targetNamespace="http://www.example.org/NewWSDLFile">

  <wsdl:types>
    <xsd:schema targetNamespace="http://www.example.org/NewWSDLFile">
    
      <!--weather app methods-->
      <!--method to get Weather by CityName and correlationID REQUEST-->
      <xsd:element name="getWeather">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="cityWithSelectionName" nillable="true" type="xsd:string"/>
            <xsd:element name="correlationId" nillable="true" type="xsd:int"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      
      <!---method to get Weather by CityName and correlationID RESPONSE-->
      <xsd:element name="getWeatherResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="temperature" nillable="true" type="xsd:double"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

  	  <!---method to get Error -->
      <xsd:element name="getWeatherError">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="errorName" nillable="true" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:schema>
  </wsdl:types>
  
  <wsdl:message name="getWeatherRequest">
    <wsdl:part element="tns:getWeather" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="getWeatherResponse">
    <wsdl:part element="tns:getWeatherResponse" name="parameters"/>
  </wsdl:message>
   <wsdl:message name="getWeatherError">
    <wsdl:part element="tns:getWeatherError" name="error"/>
  </wsdl:message>
  
  <wsdl:portType name="NewWSDLFile">
    <wsdl:operation name="getWeather">
      <wsdl:input message="tns:getWeatherRequest"/>
      <wsdl:output message="tns:getWeatherResponse"/>
      <wsdl:fault name="InputFault" message="tns:getWeatherError"/>
    </wsdl:operation>
  </wsdl:portType>
  
  <wsdl:binding name="NewWSDLFileSOAP" type="tns:NewWSDLFile">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="getWeather">
      <soap:operation soapAction="http://www.example.org/NewWSDLFile/getWeather"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
      <wsdl:fault name="InputFault">
        <soap:fault name="InputFault" use="literal"/>
      </wsdl:fault>
    </wsdl:operation>
  </wsdl:binding>
  
 <wsdl:service name="NewWSDLFile">
    <wsdl:port binding="tns:NewWSDLFileSOAP" name="NewWSDLFileSOAP">
      <soap:address location="http://www.example.org/"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>

